pipeline {
    agent any
    parameters {
            string(name: 'APP_URL', defaultValue: '', description: 'APP URL')
        }
    environment {
      ABC="abc"
 }
    stages {
      stage('NEW') {
        environment {
         XYZ="xzy"
        }
            steps {
                echo 'Hello World'
                echo 'welcome to jenkins'
                echo "${XYZ}"
                echo "${ABC}"
                echo "${APP_URL}"

            }
        }
        stage('Example') {
            input {
                message "Should we continue?"
                ok "Yes, we should."
                submitter "admin"
                }
            steps {
                echo "Hello, nice to meet you."
            }
        }
    }
    post {
      always {
        echo 'OK'
      }
    }
 }