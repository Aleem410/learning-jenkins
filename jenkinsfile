pipeline {
    agent any
    environment {
      ABC="abc"
    environment {
      SSH_PASSWORD=credentials("ssh")
    }
 }
    stages {
        stage('NEW') {
        environment {
         XYZ="xzy"

        }
            steps {
                echo 'Hello World'
                echo 'welcome to jenkins'
                echo "${XYZ}"
                echo "${ABC}"
                echo "${SSH_PASSWORD}"

            }
        }
    }
    post {
      always {
        echo 'OK'
      }
    }
}



