pipeline {
    agent any
    environment {
      ABC="abc"

 }
    stages {
        stage('NEW') {
        environment {
         XYZ="xzy"
        environment {
         ssh = credentials('ssh')
         }
        }
            steps {
                echo 'Hello World'
                echo 'welcome to jenkins'
                echo "${XYZ}"
                echo "${ABC}"
                echo "${ssh}"

            }
        }
    }
    post {
      always {
        echo 'OK'
      }
    }
}



