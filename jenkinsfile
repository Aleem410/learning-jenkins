pipeline {
    agent any
    parameters {
            string(name: 'APP_URL', defaultValue: '', description: 'APP URL')
        }
    triggers {
//     cron('0 * * * *')
       pollSCM('* * * * *')
    }
    environment {
      ABC="abc"
 }
    stages {
        stage('NEW') {
        environment {
         XYZ="xzy"

        }
            steps {
                echo 'Hello World'
                echo 'welcome to jenkins'
                echo "${XYZ}"
                echo "${ABC}"
                echo "${APP_URL}"


            }
        }
    }
    post {
      always {
        echo 'OK'
      }
    }
}



